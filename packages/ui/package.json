{
  "name": "@misterisaak/ui",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./components": {
      "development": "./src/components/index.ts",
      "default": "./dist/components.mjs"
    },
    "./composables": {
      "development": "./src/composables/index.ts",
      "default": "./dist/composables.mjs"
    },
    "./utilities": {
      "development": "./src/utilities/index.ts",
      "default": "./dist/utilities.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "unplugin-vue": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  }
}
